{
  "Non blank space": {
    "prefix": "nbs",
    "body": ["&nbsp;"]
  },
  "Console log": {
    "prefix": "log",
    "body": ["console.log($1)$0"]
  },
  "Constant": {
    "prefix": "co",
    "body": ["const $1 = $0;"]
  },
  "Class": {
    "prefix": "cls",
    "body": ["class $1 {", "\t$0", "}"]
  },
  "Class constructor": {
    "prefix": "new",
    "body": ["constructor ($1) {", "\t$0", "}"]
  },
  "Static Method": {
    "prefix": "stc",
    "body": ["static $1($2) {", "\t$0", "}"]
  },
  "Arrow function": {
    "prefix": "arr",
    "body": ["($1) => $0"]
  },
  "Function": {
    "prefix": "fun",
    "body": ["function $1($2) {", "\t$0", "}"]
  },
  "Async function": {
    "prefix": "afun",
    "body": ["async function $1($2) {", "\t$0", "}"]
  },
  "Import": {
    "prefix": "imp",
    "body": ["import {$0} from \"$1\";"]
  },
  "Describe": {
    "prefix": "desc",
    "body": ["describe(\"$1\", () => {", "\t$0", "});"],
    "description": "Describe block"
  },
  "Before Each": {
    "prefix": "be",
    "body": ["beforeEach($1() => {", "\t$0", "});"]
  },
  "It": {
    "prefix": "it",
    "body": ["it(\"$1\", () => {", "\t$0", "});"]
  },
  "It todo": {
    "prefix": "itt",
    "body": ["it.todo(\"$0\");"]
  },
  "Expect": {
    "prefix": "exp",
    "body": ["expect($1).$0;"]
  },
  "React Const Component": {
    "description": "Declare a React component",
    "prefix": "rcc",
    "body": [
      "import React from \"react\"",
      "",
      "const $1 = ($2) => {",
      "\t$0",
      "};",
      "",
      "export default $1;"
    ]
  }
}
